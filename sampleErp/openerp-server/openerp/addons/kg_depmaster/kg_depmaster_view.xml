            <field name="name">kg.depmaster.list</field>
            <field name="model">kg.depmaster</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <tree string="Dep Master">
                    <field name="name"/>
					<field name="dep_name"/>
					<field name="cost"/>
                </tree>
            </field>
        </record>
