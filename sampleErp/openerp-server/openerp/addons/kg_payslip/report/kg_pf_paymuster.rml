  <story>
<pto>
	
    <para style="terp_header_Centre">[[ (data['form']['company']) ]]</para>
    <para style="terp_header_Centre">[[ (data['form']['pay']) ]] - PF Employee Salary Muster For Month Of [[ (data['form']['month']) ]]</para>

    
    <pto_header>
      <blockTable colWidths="30,80,100,60,60,60,60,50,75" repeatRows="1" style="Table4">
      <tr>
        <td>          <para style="terp_61t">S.No.</para>        	</td>
        <td>          <para style="terp_61t">Employee Code</para>   </td>
        <td>          <para style="terp_61t">Employee Name</para>   </td>
        <td>          <para style="terp_61t">Worked Days</para>     </td>
        <td>          <para style="terp_61t">Basic Salary</para>    </td>
        <td>          <para style="terp_61t">Earned Basic</para>    </td>
        <td>          <para style="terp_61t">PF Deduction</para>    </td>
        <td>          <para style="terp_61t">Net Salary</para>      </td>
		<td>          <para style="terp_61t">Signature</para>       </td>      

        
      </tr>
    </blockTable>
   </pto_header>

   <blockTable colWidths="30,80,100,60,60,60,60,50,75" repeatRows="1" style="Table4">
      <tr>
        <td>          <para style="terp_61t">S.No.</para>        	</td>
        <td>          <para style="terp_61t">Employee Code</para>   </td>
        <td>          <para style="terp_61t">Employee Name</para>   </td>
        <td>          <para style="terp_61t">Worked Days</para>     </td>
        <td>          <para style="terp_61t">Basic Salary</para>    </td>
        <td>          <para style="terp_61t">Earned Basic</para>    </td>
        <td>          <para style="terp_61t">PF Deduction</para>    </td>
        <td>          <para style="terp_61t">Net Salary</para>      </td>
		<td>          <para style="terp_61t">Signature</para>       </td>
       
      </tr>
    </blockTable>

	<para> <seqReset id="prod_serial_no"/> </para>

    <section>
		
    <para style="terp_default_8">[[ repeatIn(get_data(data['form']), 'x') ]]</para>

	<blockTable colWidths="30,80,100,60,60,60,60,50,75" repeatRows="1" style="Table4">
      <tr>
        <td>          <para style="terp_61"> [[ x['ser_no'] ]] </para>        </td>
		<td> <para style="terp_left_61"><font>[[ (x['type']==True and setTag('para','para',{'fontName':"Helvetica-Bold"})) or removeParentNode('font') ]]</font><font>[[ x['code'] ]]</font> </para></td>
        <td>          <para style="terp_left_61">[[ x['emp_name'] ]]</para>        </td>
        <td>          <para style="terp_left_61">[[ x['worked'] ]]</para>        </td>
        <td>          
			<para style="terp_left_61">[[ locale.format('%.2f', x['basic']) ]]</para>        
			<para style="terp_left_61_bold">[[ locale.format('%.2f', x['sub_basic']) ]]</para>
        </td>        
        <td>          
			<para style="terp_left_61">[[ locale.format('%.2f', x['slip_basic']) ]]</para>
			<para style="terp_left_61_bold">[[ locale.format('%.2f', x['sub_ear']) ]]</para>
        </td>        
        <td>          
			<para style="terp_left_61">[[ locale.format('%.2f', x['pf_amt']) ]]</para>
			<para style="terp_left_61_bold">[[ locale.format('%.2f', x['sub_pf']) ]]</para>
		</td>        
        <td>          
			<para style="terp_left_61">[[ locale.format('%.2f', x['slip_basic'] - x['pf_amt']) ]]</para>        
			<para style="terp_left_61_bold">[[ locale.format('%.2f', x['sub_net'])  ]]</para>        
        </td>        
        <td>          <para style="terp_left_61"></para>        </td>        
      </tr>
    </blockTable>
	</section>    
        
